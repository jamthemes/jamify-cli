    "deepmerge": "^4.2.2",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^8.1.0",
    "jsdom": "^16.2.0",
    "jsdom-no-contextify": "^3.1.0",
    "ncp": "^2.0.0",
    "react-dom": "^15.4.1",
