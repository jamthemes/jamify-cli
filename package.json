  "license": "GPL-2.0",
  "repository": {
    "url": "https://github.com/jamthemes/jamify-cli.git"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/babel__core": "^7.1.6",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.3",
    "@types/jsdom": "^16.1.1",
    "@types/ncp": "^2.0.3",
    "@types/node": "^14.0.6",
    "@types/recursive-readdir": "^2.2.0",
    "@types/uuid": "^7.0.2",
    "@types/yargs": "^15.0.5",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.8",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
