  "main": "dist/index.js",
  "bin": {
    "jamify": "dist/index.js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --config ./nodemon_config.json --watch src --ext ts --ignore src/**/*.test.ts --exec ts-node src/index.ts",
